urguerButtonIsActive(false);\n      setIsSidebarActive('100%');\n    } else {\n      setHamburguerButtonIsActive(true);\n      setIsSidebarActive('0%');\n    }\n  }\n\n  useEffect(() => {\n    if (screen.width <= mobileScreen) {\n      setisMobile(true);\n    } else {\n      setisMobile(false);\n    }\n  }, []);\n\n  return (\n    <>\n      <NavbarContainer\n        backgroundColor={backGroundColor}\n        boxShadow={boxShadow}\n        height={navHeight}\n      >\n        <a href=\"#Home\">\n          <ContainerImage alt=\"My page icon\" src={MyPageIcon} />\n        </a>\n        <NavbarList>\n          <NavbarItem>\n            <ItemLink href=\"#Soundtrack\"> Soundtrack </ItemLink>\n          </NavbarItem>\n          <NavbarItem>\n            <ItemLink href=\"#Experiences\"> Experiences </ItemLink>\n          </NavbarItem>\n          <NavbarItem>\n            <ItemLink href=\"#Home\"> Home </ItemLink>\n          </NavbarItem>\n        </NavbarList>\n        { isMobile\n        && (\n          <>\n            <HamburgerArrow\n              isActive={hamburguerButtonIsActive}\n              toggleButton={toggleButton}\n              buttonWidth={30}\n              buttonStyle={{ zIndex: 11 }}\n            />\n            <SidebarContainer transform={isSidebarActive}>\n              <SidebarItem onClick={() => { setIsSidebarActive('100%'); setHamburguerButtonIsActive(false); }}>\n                <ItemLink href=\"#Home\"> Home </ItemLink>\n              </SidebarItem>\n              <SidebarItem onClick={() => { setIsSidebarActive('100%'); setHamburguerButtonIsActive(false); }}>\n                <ItemLink href=\"#Experiences\"> Experiences </ItemLink>\n              </SidebarItem>\n              <SidebarItem onClick={() => { setIsSidebarActive('100%'); setHamburguerButtonIsActive(false); }}>\n                <ItemLink href=\"#Soundtrack\"> Soundtrack </ItemLink>\n              </SidebarItem>\n            </SidebarContainer>\n          </>\n        )}\n      </NavbarContainer>\n    </>\n\n  );\n}\n\nexport default Navbar;\n","/home/giovani/Desktop/Projects/MyProject/MyProject-Client/src/components/Navbar/styles.ts",[],"/home/giovani/Desktop/Projects/MyProject/MyProject-Client/src/global/globalStyle.ts",[],"/home/giovani/Desktop/Projects/MyProject/MyProject-Client/src/components/Footer/index.ts",[],"/home/giovani/Desktop/Projects/MyProject/MyProject-Client/src/components/Footer/Footer.tsx",[],"/home/giovani/Desktop/Projects/MyProject/MyProject-Client/src/views/Experiences/index.ts",[],"/home/giovani/Desktop/Projects/MyProject/MyProject-Client/src/views/Experiences/Experiences.tsx",[],"/home/giovani/Desktop/Projects/MyProject/MyProject-Client/src/views/Soundtrack/index.ts",[],"/home/giovani/Desktop/Projects/MyProject/MyProject-Client/src/views/Soundtrack/Soundtrack.tsx",[],"/home/giovani/Desktop/Projects/MyProject/MyProject-Client/src/views/Soundtrack/styles.ts",[],"/home/giovani/Desktop/Projects/MyProject/MyProject-Client/src/components/Footer/styles.ts",[],"/home/giovani/Desktop/Projects/MyProject/MyProject-Client/src/views/Experiences/styles.ts",[],"/home/giovani/Desktop/Projects/MyProject/MyProject-Client/src/views/Home/styles.ts",[],{"ruleId":"91","replacedBy":"92"},{"ruleId":"93","replacedBy":"94"},{"ruleId":"95","replacedBy":"96"},{"ruleId":"97","replacedBy":"98"},{"ruleId":"99","replacedBy":"100"},{"ruleId":"101","replacedBy":"102"},{"ruleId":"103","severity":1,"message":"104","line":4,"column":10,"nodeType":"105","messageId":"106","endLine":4,"endColumn":23},"lines-around-directive",["107"],"no-spaced-func",["108"],"global-require",[],"no-buffer-constructor",[],"no-new-require",[],"no-path-concat",[],"@typescript-eslint/no-unused-vars","'FlexContainer' is defined but never used.","Identifier","unusedVar","padding-line-between-statements","func-call-spacing"]